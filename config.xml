<?xml version='1.0' encoding='utf-8'?>
<widget android-packageName="net.uslegalservices.memberapp" android-versionCode="4" ios-CFBundleIdentifier="net.uslegalservices.memberapp" ios-CFBundleVersion="8" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>U.S. Legal Services</name>
    <description>
        U.S. Legal Member Application
    </description>
    <author email="amanda@uslegal.com" href="http://uslegalservices.net">
        U.S. Legal Services
    </author>
    <content src="index.html" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="pgb-builder-version" value="2" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <platform name="android">
        <icon density="ldpi" src="resources/android/icon/ldpi.png" />
        <icon density="mdpi" src="resources/android/icon/mdpi.png" />
        <icon density="hdpi" src="resources/android/icon/hdpi.png" />
        <icon density="xhdpi" src="resources/android/icon/xhdpi.png" />
        <icon density="xxhdpi" src="resources/android/icon/xxhdpi.png" />
        <icon density="xxxhdpi" src="resources/android/icon/xxxhdpi.png" />
        <splash density="land-ldpi" src="resources/android/splash/splash-land-ldpi.png" />
        <splash density="land-mdpi" src="resources/android/splash/splash-land-mdpi.png" />
        <splash density="land-hdpi" src="resources/android/splash/splash-land-hdpi.png" />
        <splash density="land-xhdpi" src="resources/android/splash/splash-land-xhdpi.png" />
        <splash density="land-xxhdpi" src="resources/android/splash/splash-land-xxhdpi.png" />
        <splash density="land-xxxhdpi" src="resources/android/splash/splash-land-xxxhdpi.png" />
        <splash density="port-ldpi" src="resources/android/splash/splash-port-ldpi.png" />
        <splash density="port-mdpi" src="resources/android/splash/splash-port-mdpi.png" />
        <splash density="port-hdpi" src="resources/android/splash/splash-port-hdpi.png" />
        <splash density="port-xhdpi" src="resources/android/splash/splash-port-xhdpi.png" />
        <splash density="port-xxhdpi" src="resources/android/splash/splash-port-xxhdpi.png" />
        <splash density="port-xxxhdpi" src="resources/android/splash/splash-port-xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <icon height="1024" src="resources/ios/icon/icon-1024.png" width="1024" />
        <icon height="29" src="resources/ios/icon/icon-small.png" width="29" />
        <icon height="58" src="resources/ios/icon/icon-small@2x.png" width="58" />
        <icon height="87" src="resources/ios/icon/icon-small@3x.png" width="87" />
        <icon height="40" src="resources/ios/icon/icon-small-40.png" width="40" />
        <icon height="80" src="resources/ios/icon/icon-small-40@2x.png" width="80" />
        <icon height="120" src="resources/ios/icon/icon-small-40@3x.png" width="120" />
        <icon height="50" src="resources/ios/icon/icon-small-50.png" width="50" />
        <icon height="100" src="resources/ios/icon/icon-small-50@2x.png" width="100" />
        <icon height="57" src="resources/ios/icon/icon.png" width="57" />
        <icon height="114" src="resources/ios/icon/icon@2x.png" width="114" />
        <icon height="60" src="resources/ios/icon/icon-60.png" width="60" />
        <icon height="120" src="resources/ios/icon/icon-60@2x.png" width="120" />
        <icon height="180" src="resources/ios/icon/icon-60@3x.png" width="180" />
        <icon height="72" src="resources/ios/icon/icon-72.png" width="72" />
        <icon height="144" src="resources/ios/icon/icon-72@2x.png" width="144" />
        <icon height="76" src="resources/ios/icon/icon-76.png" width="76" />
        <icon height="152" src="resources/ios/icon/icon-76@2x.png" width="152" />
        <icon height="167" src="resources/ios/icon/icon-167.png" width="167" />
        <icon height="167" src="resources/ios/icon/icon-83.5@2x.png" width="167" />
        <splash src="resources/ios/splash/Default@2x~iphone~anyany.png" />
        <splash src="resources/ios/splash/Default@2x~iphone~comany.png" />
        <splash src="resources/ios/splash/Default@3x~iphone~anyany.png" />
        <splash src="resources/ios/splash/Default@3x~iphone~comany.png" />
        <splash src="resources/ios/splash/Default@2x~ipad~anyany.png" />
        <splash src="resources/ios/splash/Default@2x~ipad~comany.png" />
    </platform>
    <access origin="http://108.166.9.91" />
    <access origin="http://*.uslegalservices.net" />
    <access origin="https://*.uslegalservices.net" />
    <access origin="https://secure.identityforce.com" />
    <access launch-external="yes" origin="tel:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="https://www.uslegalservices.net/*" />
    <allow-navigation href="https://www.uslegalservices.net/*" />
    <allow-navigation href="https://secure.identityforce.com/*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="google-services.json" target="app/google-services.json" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <resource-file src="GoogleService-Info.plist" />
        <hook src="update_pods.sh" type="before_platform_add" />
        <hook src="run_pods.sh" type="after_platform_add" />
    </platform>
    <preference name="android-build-tool" value="gradle" />
    <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
        <string>App requires camera access to take pictures of tickets and road-side inspections</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
        <string>App requires access to photo library to allow users select pictures of tickets and road-side inspections</string>
    </edit-config>
    <plugin name="cordova-plugin-statusbar" spec="^2.3.0" />
    <plugin name="cordova-plugin-vibration" spec="~2.1.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.1" />
    <plugin name="phonegap-plugin-push" spec="~2.1.3">
        <variable name="FCM_VERSION" value="11.6.2" />
    </plugin>
    <plugin name="cordova-plugin-dialogs" spec="~2.0.1" />
    <plugin name="cordova-plugin-camera" spec="^2.4.1" />
    <plugin name="cordova-plugin-device" spec="^1.1.7" />
    <plugin name="cordova-plugin-device-orientation" spec="^1.0.7" />
    <plugin name="cordova-plugin-file" spec="^4.3.3" />
    <plugin name="cordova-plugin-globalization" spec="^1.0.9" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.3.0" />
    <plugin name="cordova-plugin-network-information" spec="^1.2.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^3.2.2" />
    <engine name="browser" spec="^5.0.1" />
    <engine name="ios" spec="^4.4.0" />
    <engine name="android" spec="^7.1.4" />
</widget>
