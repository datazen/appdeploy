    <access origin="http://108.166.9.91" />
    <access origin="http://*.uslegalservices.net" />
    <access origin="https://*.uslegalservices.net" />
    <access origin="https://secure.identityforce.com" />
    <access launch-external="yes" origin="tel:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="https://www.uslegalservices.net/*" />
    <allow-navigation href="https://www.uslegalservices.net/*" />
    <allow-navigation href="https://secure.identityforce.com/*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="google-services.json" target="app/google-services.json" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <resource-file src="GoogleService-Info.plist" />
    </platform>
    <preference name="android-build-tool" value="gradle" />
    <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
        <string>App requires camera access to take pictures of tickets and road-side inspections</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
        <string>App requires access to photo library to allow users select pictures of tickets and road-side inspections</string>
    </edit-config>
    <plugin name="cordova-plugin-statusbar" spec="^2.3.0" />
    <plugin name="cordova-plugin-vibration" spec="~2.1.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.1" />
